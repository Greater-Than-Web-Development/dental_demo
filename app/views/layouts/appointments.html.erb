<!DOCTYPE html>
<html>
<head>
  <title>Dental</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
  <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
  <link href="//maxcdn.bootstrapcdn.com/bootswatch/3.1.1/cerulean/bootstrap.min.css" rel="stylesheet">
  <!--   Datepicker -->
  <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.js"></script>
  <link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker.min.css" rel="stylesheet">

  <!--   Dropdown Hover Menu -->
  <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-hover-dropdown/2.0.10/bootstrap-hover-dropdown.min.js"></script>

  <!--   Chosen for Time Drop Down Multiple Selector -->

  <!-- CSS -->
  <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/chosen/1.1.0/chosen.min.css">

  <!-- JS -->
  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/chosen/1.1.0/chosen.jquery.min.js"></script>


</head>
<body>
  <%= render partial: "shared/navbar" %>

  <% if flash[:error] %>
  <div class="col-lg-6" id="errors">
    <h3 class="errors", style="color:red; font-weight: bold;"><%= flash[:error] %></h3>
  </div>
  <% end %>

  <%= yield %>

  <%= render partial: "shared/alternate-footer" %>


</body>

<!-- DatePicker -->
<script type="text/javascript">
$(document).ready(function(){
  $('[data-behaviour~=datepicker]').datepicker({
    daysOfWeekDisabled: [6,0],
    orientation: "top",
    todayBtn: true,
    todayHighlight: true
  });

})
</script>

<!-- Chosen JS -->
<script type="text/javascript">

$(document).ready(function(){
  $(".chosen-dropdown").chosen()
});

</script>

</html>


